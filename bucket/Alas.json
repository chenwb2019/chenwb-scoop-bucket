{
    "version": "0.4.9",
    "description": "Azur Lane Auto Script - A tool to automate tasks in the game Azur Lane",
    "homepage": "https://github.com/LmeSzinc/AzurLaneAutoScript",
    "license": "GPL-3.0-only",
    "url":"https://github.com/LmeSzinc/AzurLaneAutoScript/releases/latest/download/AlasApp_0.4.9_fullcn.7z",
    "hash":"sha256:89c98c4388daaa71578c5bf03c5eaeda68202ad92704b7b020a5cea0995cef4e",
    "installer": {
        "script":[
            "$inners = Get-ChildItem -Path \"$dir\" -Force | Where-Object { $_.PSIsContainer }",
            "foreach ($inner in $inners) { Get-ChildItem -Path \"$($inner.FullName)\\*\" -Force | Move-Item -Destination \"$dir\" -Force; Remove-Item -Path \"$($inner.FullName)\" -Recurse -Force }"
        ]
    },
    "bin": "Alas.exe",
    "shortcuts": [
        [
            "Alas.exe",
            "Alas"
        ]
    ],
    "persist":[
        "config"
    ]
}

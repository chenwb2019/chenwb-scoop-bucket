{
    "version": "1.8",
    "description": "A tool to manage and organize your desktop icons.",
    "homepage": "https://github.com/Twometer/NoFences",
    "license": "MIT",
    "notes": "run 'reg import install_startup.reg' to enable startup, 'reg import uninstall_startup.reg' to disable it",
    "url": "https://github.com/Twometer/NoFences/releases/download/v1.8/NoFences.exe",
    "hash": "c68515e759f13a6af53c6c83cf14894e5dad1f22ab5b3c3a4d81ad7425105e0f",
    "bin": "NoFences.exe",
    "shortcuts": [["NoFences.exe", "NoFences", "", "NoFences.exe"]],
    "post_install": [
        "$cc_root = \"$dir\".Replace('\\', '\\\\')",
        ". \"$bucketsdir\\chenwb-scoop-bucket\\scripts\\Functions.ps1\"",
        "Add-Startup_menu -cc_root $cc_root -exeName 'NoFences.exe' -shortcutName 'NoFences'",
        "Move-Data_dir -data_dir \"$env:LOCALAPPDATA\\NoFences\" -persist_dir $persist_dir"
    ],
    "pre_uninstall": [
        "reg import \"$dir\\uninstall_startup.reg\" 2>&1 | Out-Null",
        "stop-process -name NoFences -ErrorAction SilentlyContinue",
        "Remove-Item \"$env:LOCALAPPDATA\\NoFences\" -Recurse -Force -ErrorAction SilentlyContinue"
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/Twometer/NoFences/releases/download/v$version/NoFences.exe"
    }
}
